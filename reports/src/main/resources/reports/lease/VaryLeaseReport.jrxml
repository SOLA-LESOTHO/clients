	<detail>
		<band height="144" splitType="Stretch">
			<staticText>
				<reportElement uuid="cef82f44-9df7-40e2-add1-047101e192ff" x="14" y="12" width="528" height="20"/>
				<textElement>
					<font fontName="Calibri" size="11" isBold="true" isUnderline="true"/>
				</textElement>
				<text><![CDATA[To Whom it May Concern That:]]></text>
			</staticText>
			<textField isStretchWithOverflow="true">
				<reportElement uuid="61a180ea-5caf-4731-914f-d6c6236511b0" positionType="Float" x="14" y="42" width="528" height="20"/>
				<textElement markup="html">
					<font fontName="Calibri" size="11" isBold="false" isUnderline="false"/>
				</textElement>
				<textFieldExpression><![CDATA[String.format(
"(i)	This deed is supplemental/compensation to lease No <b>%s</b>  as held by <b>%s</b>"+
"<p></p>"+
"(ii)	The Minister of Local Government and Chieftainship has agreed on <b>VARIATION</b>"+
"  pursuant to Section 42 of The Land Act 2010 to the variation of the lease No %s  in the following manner:",
$F{parcelCode},
$F{lessees},
$F{parcelCode}
)]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement uuid="37f8f830-7edf-4d7e-b37c-52b055a002c5" positionType="Float" x="46" y="72" width="496" height="20"/>
				<textElement markup="html">
					<font fontName="Calibri" size="11" isBold="false" isUnderline="false"/>
				</textElement>
				<textFieldExpression><![CDATA[String.format(
"(a)	The purpose for which the leased land shall be used is <b>%s</b>"+
"<p></p>" +
"(b)	The annual ground rent now reserved under this lease shall mean Maloti <b>%s</b> "+ "effective from the  <b>%s</b> day of <b>%s</b>"+
"<p></p>" +
"(c)	The lease shall now expire at the end of a  <b>%s (%s)</b> year term to be measured "+ 
"from the <b>%s</b>  day of <b>%s</b>." +
"<p></p>",
$F{parcelLandUse},
$F{groundRent}, $F{leaseStartDay}, $F{leaseStartMonthAndYear}, 
$F{leaseTermWord}, $F{leaseTerm}, 
$F{leaseExecutionDay}, $F{leaseExecutionMonthAndYear}
)]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement uuid="1b3cc18d-7dff-47b8-a4ec-960c1a6cf2c9" positionType="Float" x="14" y="105" width="528" height="20"/>
				<textElement markup="html">
					<font fontName="Calibri" size="11" isBold="false" isUnderline="false"/>
				</textElement>
				<textFieldExpression><![CDATA[String.format(
"Other Conditions" +
"<p></p>" +
"(iii)	No variations shall be deemed to affect the said Lease No <b>%s</b>  unless expressly stated in this deed.",
$F{parcelCode}
)]]></textFieldExpression>
			</textField>
