				<textFieldExpression><![CDATA[String.format("Other special conditions: <b>%s</b>",$F{specialConditions})]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement uuid="70c683a1-3136-417e-a735-34873c5f8747" positionType="Float" x="37" y="129" width="471" height="59"/>
				<textElement>
					<font fontName="Times New Roman" size="11" isBold="false" isPdfEmbedded="true"/>
				</textElement>
				<text><![CDATA[That this lease shall only be terminated by:
(i)    the expiry of its period; or
(ii)   the surrender thereof under the provisions of Section 35; or
(iii)  its determination pursuant to the provisions of the Act.]]></text>
			</staticText>
			<staticText>
				<reportElement uuid="cf1d13c9-fdd6-4783-ae2a-4afaf0f86a2c" positionType="Float" x="0" y="129" width="37" height="20"/>
				<textElement>
					<font fontName="Times New Roman" size="11" isBold="false" isPdfEmbedded="true"/>
				</textElement>
